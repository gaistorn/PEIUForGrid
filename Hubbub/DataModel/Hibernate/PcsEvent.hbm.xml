<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="DataModel" namespace="DataModel" xmlns="urn:nhibernate-mapping-2.2">
  <class name="PcsEvent" table="pcs_event" lazy="true" >
    <id name="Uniqueid" column="uniqueid" />
    <many-to-one name="Deviceinfo">
      <column name="deviceinfo" sql-type="int(11)" not-null="true" />
    </many-to-one>
    <property name="Timestamp">
      <column name="timestamp" sql-type="datetime" not-null="true" />
    </property>
    <property name="Message">
      <column name="message" sql-type="varchar(255)" not-null="true" />
    </property>
    <property name="Isack">
      <column name="isack" sql-type="bit(1)" not-null="true" />
    </property>
    <property name="Acktimestamp">
      <column name="ackTimestamp" sql-type="datetime" not-null="false" />
    </property>
    <property name="Ackuser">
      <column name="ackUser" sql-type="varchar(45)" not-null="false" />
    </property>
    <property name="Category">
      <column name="category" sql-type="varchar(45)" not-null="true" />
    </property>
    <property name="Isfault">
      <column name="IsFault" sql-type="bit(1)" not-null="true" />
    </property>
  </class>
</hibernate-mapping>