<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="DataModel" namespace="DataModel" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Deviceinfo" table="deviceinfo" lazy="true" >
    <id name="Uniqueid" column="uniqueId" />
    <property name="Devicename">
      <column name="deviceName" sql-type="varchar(45)" not-null="true" />
    </property>
    <bag name="BatInfos">
      <key column="uniqueId" />
      <one-to-many class="BatInfo" />
    </bag>
    <bag name="PcsEvent">
      <key column="" />
      <one-to-many class="PcsEvent" />
    </bag>
    <bag name="PcsSystem">
      <key column="" />
      <one-to-many class="PcsSystem" />
    </bag>
  </class>
</hibernate-mapping>