<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="DataModel" namespace="DataModel" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ModbusSystem" table="ModbusSystem" lazy="false" >
    <id name="Id" column="ID" />
    <property name="DeviceName">
      <column name="DeviceName" sql-type="TEXT " not-null="true" />
    </property>
    <property name="IpAddress">
      <column name="IpAddress" sql-type="STRING " not-null="true" />
    </property>
    <property name="Disable">
      <column name="Disable" sql-type="BOOLEAN" not-null="true" />
    </property>
    <property name="PortNum">
      <column name="PortNum" sql-type="INT" not-null="true" />
    </property>
    <bag name='GroupPoints' table='GroupPoint' lazy='false'>
      <key column='ModbusId'/>
      <one-to-many class='GroupPoint'/>
    </bag>
    <bag name='GroupDigitalPoints' table='EventGroupPoint' lazy='false'>
      <key column='ModbusId'/>
      <one-to-many class='EventGroupPoint'/>
    </bag>
    
  </class>
</hibernate-mapping>